page-tabs {
    super-tabs.super-tab-menu super-tab-button {
        display: block !important;
        white-space: inherit !important;
        padding: 15px 5px !important;
        font-size: 13px !important;
        color: #878787;
        font-weight: 400 !important;
        font-family: 'Muli', sans-serif;
        text-transform: capitalize !important;
        border-right: 1px solid #e4e4e4;
    }
 
    .title-and-icon.selected{
        font-size: 13px !important;
        font-weight: 600 !important;
        color: #13a89e !important;
        font-family: 'Muli', sans-serif !important;
        text-decoration: none !important;
        border-bottom: 2px solid #13a89e !important;
        
    }
    .title-and-icon.selected ion-icon.icon.icon{
        font-size: 18px !important;
        color: #13a89e !important;
        margin-bottom: 5px !important;
    }
    super-tabs.super-tab-menu super-tab-button:last-child {
        border-right: none;
    }
    .button-md-primary {
        color: #fff;
        background-color: #13a89e;
    }
    .header-md::after, .tabs-md[tabsPlacement="top"] > .tabbar::after, .footer-md::before, .tabs-md[tabsPlacement="bottom"] > .tabbar::before {
        left: 0;
        bottom: -5px;
        background-position: left 0 top -2px;
        position: absolute;
        width: 100%;
        height: 5px;
        background-image: none;
        background-repeat: repeat-x;
        content: "";
    }
    ion-navbar.toolbar {
        border-bottom: 1px solid #e4e4e4;
    }
    super-tab-button ion-icon.icon.icon {
        font-size: 18px !important;
        color: #3d3d3d;
        margin-bottom: 5px !important;
    }
}
