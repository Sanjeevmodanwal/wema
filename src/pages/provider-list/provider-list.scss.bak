.img {
    width: 90px;
    height: 90px;
    margin: 0 auto;
    background: #fff;
    padding: 10px;
    border-radius: 100%;
    overflow: hidden;
    position: relative;
}
.img img{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 70px;
}
ion-grid.header-part.grid h1{
    color: #fff;
    font-size: 16px;
    font-family: 'Muli', sans-serif;
    font-weight: bold;
    margin-bottom: 3px;
    margin-top: 10px;
}
ion-grid.header-part.grid p{
    color: #fff;
    font-size: 13px;
    font-family: 'Muli', sans-serif;
    margin: 2px 0px;
}
ion-grid.header-part.grid h2{
    color: #fdba12;
    font-size: 16px;
   margin: 5px 0px;
}
ion-grid.header-part.grid .call-icon ion-icon{
    color: #12a89d;
    font-size: 20px;
    margin: 5px 3px;
    background: #fff;
    width: 36px;
    height: 36px;
    border-radius: 100%;
    padding: 9px;
}
.right-icon {
    position: absolute;
    right: 10px;
    top: 30px;
}
.right-icon ion-icon{
    display: block;
    font-size: 24px;
    color: #fff;
    margin-bottom: 10px;
}

