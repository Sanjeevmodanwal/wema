    height: 200px;
}
ion-grid.header-part.grid {
    text-align: center;
    background: #12a89d;
    padding: 5px 5px 15px;
    position: relative;
}
ion-navbar.dawn-bar .toolbar-background{
    background: #12a89d;
    
}
.header-md::after, .tabs-md[tabsPlacement="top"] > .tabbar::after, .footer-md::before, .tabs-md[tabsPlacement="bottom"] > .tabbar::before {
    display: none;
}
